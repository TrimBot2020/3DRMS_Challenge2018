\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Evaluation Methodology
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Test GT 
\end_layout

\begin_deeper
\begin_layout Itemize
delimited by the bounding box of the test area 
\end_layout

\end_deeper
\begin_layout Itemize
3D geometry
\end_layout

\begin_deeper
\begin_layout Itemize
Submissions cropped to bounding box +1 m
\end_layout

\begin_layout Itemize
Cumulative histograms of distances (mesh 
\begin_inset Formula $\rightleftarrows$
\end_inset

 GT points)
\end_layout

\begin_layout Itemize

\series bold
Accuracy
\series default
 is distance 
\begin_inset Formula $d$
\end_inset

 (in m) such that 90% of the reconstruction is within 
\begin_inset Formula $d$
\end_inset

 of the ground truth mesh
\end_layout

\begin_layout Itemize

\series bold
Completeness
\series default
 is the percent of points in the GT point cloud that are within 5 cm of
 the reconstruction
\end_layout

\end_deeper
\begin_layout Itemize
Semantics
\end_layout

\begin_deeper
\begin_layout Itemize
Labels assigned to vertices or faces of the 3D model
\end_layout

\begin_layout Itemize
Projected to all test images 
\end_layout

\begin_layout Itemize

\series bold
Prediction accuracy
\series default
 of the pixels corresponding to the 3D test part
\end_layout

\end_deeper
\end_deeper
